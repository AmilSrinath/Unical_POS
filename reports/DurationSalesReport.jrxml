	<parameter name="dateFrom" class="java.lang.String"/>
	<parameter name="dateTo" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
    -- 💰 Cash & Card
    FORMAT(SUM(CASE WHEN pmd.status_id NOT IN (6, 7) THEN pmd.cod_amount ELSE 0 END), 2) AS Total_Report_Cash,
    FORMAT(SUM(CASE WHEN pmd.status_id NOT IN (6, 7) THEN pmo.paid_amount ELSE 0 END), 2) AS Total_Report_Card,

    -- 📊 Total orders
    COUNT(pmo.order_id) AS Number_of_orders,

    -- 🟢 Active (1)
    COUNT(CASE WHEN pmd.status_id = 1 THEN 1 END) AS Active_orders_count,

    -- ⏳ Pending (2)
    COUNT(CASE WHEN pmd.status_id = 2 THEN 1 END) AS Pending_orders_count,

    -- 🎁 Wrapping (3)
    COUNT(CASE WHEN pmd.status_id = 3 THEN 1 END) AS Wrapping_orders_count,

    -- 📤 Despatch (4)
    COUNT(CASE WHEN pmd.status_id = 4 THEN 1 END) AS Despatch_orders_count,

    -- 📦 Delivered (5)
    COUNT(CASE WHEN pmd.status_id = 5 THEN 1 END) AS Delivered_orders_count,

    -- ❌ Return (6)
    COUNT(CASE WHEN pmd.status_id = 6 THEN 1 END) AS Return_orders_count,

    -- ❌ Cancel (7)
    COUNT(CASE WHEN pmd.status_id = 7 THEN 1 END) AS Cancel_orders_count,

    -- 🔄 Returning (12)
    COUNT(CASE WHEN pmd.status_id = 12 THEN 1 END) AS Returning_orders_count,

    -- 🔍 Checking (13)
    COUNT(CASE WHEN pmd.status_id = 13 THEN 1 END) AS Checking_orders_count,

    -- 💰 Return & Cancel totals
    FORMAT(SUM(CASE WHEN pmd.status_id = 6 THEN pmo.total_order_price ELSE 0 END), 2) AS Return_orders_Total,
    FORMAT(SUM(CASE WHEN pmd.status_id = 7 THEN pmo.total_order_price ELSE 0 END), 2) AS Cancel_orders_Total,

    -- 🚚 Delivery fee
    FORMAT(SUM(pmo.delivery_fee), 2) AS Total_delivery_charge,

    -- 💵 Grand total (Cash + Card - Return - Cancel - Delivery)
    FORMAT(
        (
            SUM(CASE WHEN pmd.status_id NOT IN (6, 7) THEN pmd.cod_amount ELSE 0 END) +
            SUM(CASE WHEN pmd.status_id NOT IN (6, 7) THEN pmo.paid_amount ELSE 0 END) -
            SUM(CASE WHEN pmd.status_id = 6 THEN pmo.total_order_price ELSE 0 END) -
            SUM(CASE WHEN pmd.status_id = 7 THEN pmo.total_order_price ELSE 0 END) -
            SUM(pmo.delivery_fee)
        ),
        2
    ) AS Grand_total,

    -- 📊 Percentages by status
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 1  THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Active_percentage,
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 2  THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Pending_percentage,
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 3  THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Wrapping_percentage,
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 4  THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Despatch_percentage,
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 5  THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Delivered_percentage,
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 6  THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Return_percentage,
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 7  THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Cancel_percentage,
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 12 THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Returning_percentage,
    CONCAT(ROUND((COUNT(CASE WHEN pmd.status_id = 13 THEN 1 END) / COUNT(pmo.order_id)) * 100, 2), '%') AS Checking_percentage

FROM
    pos_main_order_tb pmo
JOIN
    pos_main_delivery_order_tb pmd ON pmo.delivery_order_id = pmd.delivery_id
WHERE
    DATE(pmo.created_Date) BETWEEN  $P{dateFrom}  AND  $P{dateTo} ;]]>
	</queryString>
